<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

   android:background="@drawable/bg111"
     >




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical" >

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="蚂蚁企业通讯录"
            android:layout_marginTop="20dip"
            android:textSize="22dip"
            android:id="@+id/title2"
            android:textColor="@color/yellow"
            />
        <com.ant.contact.View.ReboundScrollView
            android:layout_width="match_parent"
            android:scrollbars="none"
            android:layout_marginTop="15dip"
            android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"

            android:background="@drawable/bg_white_trans">

            <com.ant.contact.View.ScrollListView android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/list_1_1"
                android:divider="@color/gray"
                android:dividerHeight="1px"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                ></com.ant.contact.View.ScrollListView>

        </LinearLayout>
        </com.ant.contact.View.ReboundScrollView>
    </LinearLayout>

</LinearLayout>